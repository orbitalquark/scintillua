+static int locale_l (lua_State *L) {
+  if (lua_isnoneornil(L, 1)) {
+    lua_settop(L, 0);
+    lua_createtable(L, 0, 12);
+  }
+  else
+    luaL_checktype(L, 1, LUA_TTABLE);
+  createcat(L, "alnum", isalnum);
